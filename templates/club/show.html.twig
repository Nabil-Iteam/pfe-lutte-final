{% extends 'base.html.twig' %}

{% block title %}Club{% endblock %}

{% block body %}
    <h1>Club</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ club.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ club.name }}</td>
            </tr>
            <tr>
                <th>ArabicName</th>
                <td>{{ club.arabicName }}</td>
            </tr>
            <tr>
                <th>Abreviation</th>
                <td>{{ club.abreviation }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ club.address }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ club.phone }}</td>
            </tr>
            <tr>
                <th>Fax</th>
                <td>{{ club.fax }}</td>
            </tr>
            <tr>
                <th>Para</th>
                <td>{{ club.para ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>ZipCode</th>
                <td>{{ club.zipCode }}</td>
            </tr>
            <tr>
                <th>AutorisationCode</th>
                <td>{{ club.autorisationCode }}</td>
            </tr>
            <tr>
                <th>FondationYear</th>
                <td>{{ club.fondationYear ? club.fondationYear|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CoachName</th>
                <td>{{ club.coachName }}</td>
            </tr>
            <tr>
                <th>CoachPhone</th>
                <td>{{ club.coachPhone }}</td>
            </tr>
            <tr>
                <th>PresidentName</th>
                <td>{{ club.presidentName }}</td>
            </tr>
            <tr>
                <th>PresidentPhone</th>
                <td>{{ club.presidentPhone }}</td>
            </tr>
            <tr>
                <th>SecretaryName</th>
                <td>{{ club.secretaryName }}</td>
            </tr>
            <tr>
                <th>TreasurerPhone</th>
                <td>{{ club.treasurerPhone }}</td>
            </tr>
            <tr>
                <th>SecretaryPhone</th>
                <td>{{ club.secretaryPhone }}</td>
            </tr>
            <tr>
                <th>TreasurerName</th>
                <td>{{ club.treasurerName }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ club.createdAt ? club.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ club.updatedAt ? club.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ club.email }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_club_index') }}">back to list</a>

    <a href="{{ path('app_club_edit', {'id': club.id}) }}">edit</a>

    {{ include('club/_delete_form.html.twig') }}
{% endblock %}
